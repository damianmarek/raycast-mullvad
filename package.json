{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mullvad",
  "title": "Mullvad",
  "description": "Control Mullvad VPN",
  "icon": "mullvad-logo.png",
  "author": "damianmarek",
  "categories": [
    "Security"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "connect",
      "title": "Connect to Mullvad",
      "description": "Connect to Mullvad VPN",
      "mode": "no-view"
    },
    {
      "name": "disconnect",
      "title": "Disconnect from Mullvad",
      "description": "Disconnect from Mullvad VPN",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.3",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
